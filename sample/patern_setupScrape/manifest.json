{
    "name": "craper",
    "description": "craper",
    "version": "1.0",
    "permissions": ["<all_urls>"],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "craper title"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "run_at":"document_end"
    }],
    "manifest_version": 2
}