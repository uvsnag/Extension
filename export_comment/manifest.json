{
    "name": "export_comment",
    "description": "export_comment",
    "version": "1.0",
    "permissions": [
        "<all_urls>"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "export_comment"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "background.js"
            ],
            "run_at": "document_end"
        }
    ],
    
    "manifest_version": 2
}